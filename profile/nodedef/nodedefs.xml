<nodeDefs>
    <!-- NODE DEF from 5.0 document http://wiki.universal-devices.com/index.php?title=V50_MAIN -->
    <!-- controller -->
    <nodeDef id="controller" nls="ctl">
        <editors />
        <sts>
			<st id="ST" editor="bool" />
		</sts>
        <cmds>
            <sends />
            <accepts>
              <cmd id="DISCOVER" />
              <cmd id="REMOVE_NOTICES_ALL" />
              <cmd id="UPDATE_PROFILE" />
            </accepts>
        </cmds>
    </nodeDef>
	<!-- mynodetype -->
    <nodeDef id="onkyozone" nls="ozone">
        <editors />
        <sts>
            <st id="ST" editor="bool" />
            <st id="SVOL" editor="svolume" />
            <st id="GV0" editor="bool" />
        </sts>
        <cmds>
            <sends />
            <accepts>
                <cmd id="VOLUME">
                       <p id="" editor="svolume" init="SVOL" />
               </cmd>
                <cmd id="MUTE" />
                <cmd id="UNMUTE" />
                <cmd id="POWERON" />
                <cmd id="POWEROFF" />
                
                
            </accepts>
        </cmds>
    </nodeDef>
</nodeDefs>
